    <!-- <div class="back-wrap"> -->
      <a href="/#blog" class="back-link proj-back"><i class="fa fa-long-arrow-left"></i> back to Portfolio</a>
    <!-- </div> -->

    <div class="container">

      <div class="project-single-wrap">

        <div class="row">
          <div class="col-sm-10 col-sm-offset-1 proj-bottom">
            <h1 class="text-center"><%= title %></h1>
            <!-- <h2 class="project-h2">A short description to place here, d'you get me?</h2> -->
            <span class="center-line"></span>
            <div class="proj-buttons-top">
              <a href="#" class="btn btn-lg btn-dark">Watch Live</a>
              <a href="#" class="btn btn-border-white btn-lg">Code on Github</a>
            </div>
          </div>
        </div>

        <div class="project-desc">

          <div class="col-sm-12 proj-bottom">
            <div class="row">
              <div class="col-sm-10 col-sm-offset-1 margin-btm-30">
                <h4>Description</h4>
                <p>
                  <%= description %>
                </p>
              </div>
            </div>
          </div>

          <div class="col-sm-12 proj-bottom">
            <div class="row">
              <div class="col-sm-10 col-sm-offset-1 margin-btm-30 technologies">
                <h4>Technologies / Third-party modules and cool features</h4>

                  <% technologies.forEach(function(tech) { %>
                    <span><%= tech %></span> /
                  <% }); %>

              </div>
            </div>
          </div>



        </div>

        <div class="col-sm-10 col-sm-offset-1 proj-bottom">
          <div class="row">
            <% images.forEach(function(imagepath) { %>
            <div class="col-sm-4 margin-btm-30">
              <div class="item-img-wrap">
                <img src="/img/proj/<%= imagepath %>" class="img-responsive projects-img" alt="workimg">
                <div class="item-img-overlay">
                  <a href="/img/proj/<%= imagepath %>" class="show-image">
                    <span></span>
                  </a>
                </div>
              </div> <!--item img wrap-->
            </div><!--work-single images-->
            <% }); %>
          </div>
        </div>



        <div class="proj-buttons-end">
          <a href="#" class="btn btn-lg btn-dark">Watch Live</a>
          <a href="#" class="btn btn-border-white btn-lg">Code on Github</a>
        </div>

      </div>

    </div>

<%- include('footer') %>
