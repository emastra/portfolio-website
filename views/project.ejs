    <!-- <div class="back-wrap"> -->
      <a href="/#portfolio" class="back-link proj-back"><i class="fa fa-long-arrow-left"></i> back to Portfolio</a>
    <!-- </div> -->

    <div class="container">

      <div class="project-single-wrap">

        <div class="row">
          <div class="col-sm-10 col-sm-offset-1 proj-bottom">
            <h1 class="text-center"><%= title %></h1>
            <h2 class="project-h2"><%= subtitle %></h2>
            <span class="center-line"></span>
            <div class="proj-buttons-top">
              <%  if (title.includes('CLI')) { %>
                <a href="<%= links.git %>" class="btn btn-border-white btn-lg" target="_blank">Code on Github</a>
              <% } else { %>
                <a href="<%= links.live %>" class="btn btn-lg btn-dark" target="_blank">View Live</a>
                <a href="<%= links.git %>" class="btn btn-border-white btn-lg" target="_blank">Code on Github</a>
              <% } %>
            </div>
          </div>
        </div>

        <div class="col-sm-10 col-sm-offset-1 proj-bottom">
          <div class="row">
            <% images.forEach(function(imagepath) { %>
            <div class="col-sm-4 margin-btm-30">
              <div class="item-img-wrap">
                <img src="/img/proj/<%= imagepath %>" class="img-responsive projects-img" alt="workimg">
                <div class="item-img-overlay">
                  <a href="/img/proj/<%= imagepath %>" class="show-image">
                    <span></span>
                  </a>
                </div>
              </div> <!--item img wrap-->
            </div><!--work-single images-->
            <% }); %>
          </div>
        </div>

        <div class="project-desc">
          <div class="col-sm-12 desc-bottom">
            <div class="row">
              <div class="col-sm-6 margin-btm-30">
                <h4>Description</h4>
                <p>
                  <%= description %>
                </p>
              </div>
              <div class="col-sm-6 margin-btm-30 technologies">
                <h4>Technologies and Features</h4>
                <div id="tech-list">
                  <% technologies.forEach(function(tech) { %>
                    <span class="tech-item"><%= tech %></span><strong>/</strong>
                  <% }); %>
                </div>
              </div>
            </div>
          </div>
        </div>

        <div class="col-sm-12">
          <div class="row proj-buttons-end">
            <%  if (title.includes('CLI')) { %>
              <a href="<%= links.git %>" class="btn btn-border-white btn-lg" target="_blank">Code on Github</a>
            <% } else { %>
              <a href="<%= links.live %>" class="btn btn-lg btn-dark" target="_blank">View Live</a>
              <a href="<%= links.git %>" class="btn btn-border-white btn-lg" target="_blank">Code on Github</a>
            <% } %>
          </div>
        </div>


      </div>

    </div>

<%- include('footer') %>
